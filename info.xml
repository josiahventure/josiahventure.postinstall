<?xml version="1.0"?>
<extension key="josiahventure.postinstall" type="module">
  <file>postinstall</file>
  <name>Post Installation</name>
  <description>Makes the postinstallation tasks repeatable</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Klaas Eikelboom</author>
    <email>klaas.eikelboom@civicoop.org</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/josiahventure/josiahventure.postinstall</url>
    <url desc="Documentation">https://github.com/josiahventure/josiahventure.postinstall/blob/master/README.md</url>
    <url desc="Support">info@josiahventure.com</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2020-02-18</releaseDate>
  <version>1.0</version>
  <develStage>alpha</develStage>
  <compatibility>
    <ver>5.0</ver>
  </compatibility>
  <comments>This extension is very specific for JosiahVenture, so use it for inspiration, not for production</comments>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/Postinstall</namespace>
  </civix>
</extension>
